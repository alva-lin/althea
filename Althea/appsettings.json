{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "logs/log.txt",
                "rollingInterval": "Day",
                "restrictedToMinimumLevel": "Information"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "Althea"
    }
  },
  "ConnectionStrings": {
    "Althea": "Server=<hostname>;Port=<port>;Database=<database>;User Id=<username>;Password=<password>;Integrated Security=false;Pooling=true;"
  },
  "JwtOption": {
    "Secret": "Althea",
    "Issuer": "Althea",
    "Audience": "Althea",
    "AccessExpiration": "3600",
    "RefreshExpiration": "1440"
  },
  "CorsOption": {
    "AllowOrigins": [
      "127.0.0.1",
      "localhost"
    ],
    "AllowHeaders": [
      "x-custom-client",
      "Authorization"
    ]
  },
  "SystemOption": {
  }
}
